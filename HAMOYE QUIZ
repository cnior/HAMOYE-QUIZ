import numpy as np
import pandas as pd
import matplotlib.pyplot as plt

df = pd.read_csv('https://raw.githubusercontent.com/WalePhenomenon/climate_change/master/fuel_ferc1.csv')
statistics = (df.describe())
print(statistics)

null_values = (df.isna().sum())
print(null_values)

head = (df.head())
print(head)

correlation = (df.corr())
print(correlation)
